{
  "name": "mongoscrape",
  "version": "1.0.0",
  "description": "Mongoscrape is an application designed to scrape a news website and save articles to a database.  Users can save articles and leave comments.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamTaub/mongoscrape.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SamTaub/mongoscrape/issues"
  },
  "homepage": "https://github.com/SamTaub/mongoscrape#readme",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.2",
    "concat": "^1.0.3",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.2",
    "mongoose": "^5.4.19",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^6.1.3"
  }
}
